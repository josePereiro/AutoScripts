{
    // ----. -.- .-... -- -... - -. -- .- .- .-.-.--.-.
    // vscode
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "terminal.integrated.fontSize": 15,
    "editor.minimap.enabled": true,
    "editor.fontSize": 18,
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 90,
    // "window.commandCenter": false,
    "workbench.editorAssociations": {
        "{git,gitlens}:/**/*.{md,csv,svg}": "default",
        "*.md": "default",
        "*.csv": "default",
        "*.pdf": "latex-workshop-pdf-hook"
    },
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt",
        "matlab.interrupt"
    ],
    "explorer.confirmDelete": false,
    "files.autoSave": "afterDelay",
    "editor.suggest.selectionMode": "never",
    "editor.suggest.preview": false,
    "editor.suggestOnTriggerCharacters": false,
    // "editor.inlineSuggest.enabled": false,
    // "editor.inlineSuggest.suppressSuggestions": true,
    "editor.quickSuggestionsDelay": 10000000,
    "editor.minimap.showSlider": "always",

    // ----. -.- .-... -- -... - -. -- .- .- .-.-.--.-.
    // proxy
    "http.proxy": "http://10.6.100.72:3128",
    // "http.proxy": "http://apereiro%40fbio.uh.cu:92110927807@proxy-trab.uh.cu:3128/",
    // "http.proxy": "http://proxy-trab.uh.cu:3128/",
    // "http.proxy": "http://10.6.230.13:58132/",
    // "http.proxy": "http://10.6.230.13:58132/",


    // ----. -.- .-... -- -... - -. -- .- .- .-.-.--.-.
    // julia
    // "julia.enableCrashReporter": false,
    // "julia.symbolCacheDownload": true,

    // ----. -.- .-... -- -... - -. -- .- .- .-.-.--.-.
    // python
    "python.venvFolders": [".python/venvs"],

    // ----. -.- .-... -- -... - -. -- .- .- .-.-.--.-.
    // git
    "git.openRepositoryInParentFolders": "never",
    
    // ----. -.- .-... -- -... - -. -- .- .- .-.-.--.-.
    // latex
    "latex-workshop.view.pdf.color.dark.backgroundColor": "#ffffff",
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.view.pdf.invert": 0.9, 
    "latex-workshop.latex.outDir": "%DIR%/.aux",
    "latex-workshop.latex.autoBuild.run": "never",

    // ----. -.- .-... -- -... - -. -- .- .- .-.-.--.-.
    // Other plugins
    // List of tags. *Note, if one tag starts with another tag, the longer 
    // tag should be specified first to prevent the shorter tag being matched.*
    "todo-tree.general.tags": [
        "BUG", "TODO", "ToRead", "IDEAS", "XXX", "TODEL", "DEV", "ToDel", "[ ]", "[x]"
    ],
    "julia.symbolCacheDownload": true,
    "explorer.confirmDragAndDrop": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "julia.enableTelemetry": false,
    "git.confirmSync": false,
    "cmake.showOptionsMovedNotification": false,
    "julia.enableCrashReporter": true,
    "security.workspace.trust.untrustedFiles": "open",
    
}



